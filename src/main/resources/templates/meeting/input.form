<div class="box ui-draggable ui-droppable" id="pm">
    <div class="box-header">
        <div class="box-name">
            <i class="fa fa-search"></i> <span>填写会议纪要</span>
        </div>
        <div class="box-icons">
            <a class="collapse-link"> <i class="fa fa-chevron-up"></i>
            </a> <a class="expand-link"> <i class="fa fa-expand"></i>
        </a> <a class="close-link"> <i class="fa fa-times"></i>
        </a>
        </div>
        <div class="no-move"></div>
    </div>
    <div class="box-content">
        <form role="form" action="" method="post">
            <div class="form-group">
                <label>主持人</label>
                <input class="form-control" id="host" name="host" value=${host} disabled="disabled" />
            </div>
            <div class="form-group">
                <label>会议主题</label>
                <input class="form-control" id="topic" name="topic" value=${place} disabled="disabled" />
            </div>
            <div class="form-group">
                <label>会议时间</label>
                <input class="form-control" id="startTime" name="startTime" value=${startTime} disabled="disabled" />
            </div>
            <div class="form-group">
                <label>会议地点</label>
                <input class="form-control" id="place" name="place" value=${place} disabled="disabled" />
            </div>
            <div class="form-group">
                <label>参与人</label>
                <input class="form-control" id="people" name="people" value=${peoplelist} disabled="disabled" />
            </div>
            <div class="form-group has-feedback">
                <label class="control-label">会议内容</label>
                <textarea id="meetingcontent" rows="10" class="form-control" name="content">
                </textarea>
            </div>
            <button id="btn" type="button" class="btn btn-default">完成</button>
        </form>
    </div>
</div>

</div>

<script type="text/javascript">
    $(document).ready(function(){
        $("#btn").click(function(){
            $.post("completeMeetingContent/"+$("form").data("taskid"),$("form").serialize(),function(a){
                alert("处理成功");
                LoadAjaxContent("meetingtask");
            });
        });
    });
</script>